'==========以下是按键精灵录制的内容==========
UserVar Shift=15 "设置内容"
UserVar N=0 "设置内容"
UserVar M=0 "设置内容"
  
While Shift>0      
	For 25
	MoveTo 180, 250+N*Shift
	Delay 1000
	LeftClick 1
	Delay 1
	LeftUp 1
	Delay 1000
	
	IfColor 216, 209, "FFFFFF", 0 Then
		M = M + 1
        MoveTo 103, 63
		LeftClick 1
		Delay 1000
		MoveTo 543, 443
		LeftClick 1
		Delay 1000
        MoveTo 382, 510
		LeftClick 1
		Delay 1000
        KeyPress "Enter", 1
        KeyPress "Left", 1
		Delay 1000
		KeyPress "Enter", 1

        Delay 1000
		KeyDown "BackSpace", 1
		Delay 1
		KeyUp "BackSpace", 1
		Delay 1
	End If
	N=N+1
	Next
	
	
	IfColor 300, 690, "0000E1", 0 Then
		MoveTo 300, 690
		Delay 1000
		LeftClick 1
		LeftUp 1
		Delay 1000
	Else 
		MoveTo 528, 682
		Delay 1000
		LeftClick 1
		LeftUp 1
		Delay 1000
	End If
	
	IfColor 530, 678, "6F6F6F", 0 Then
		EndScript
	End If	

EndWhile

